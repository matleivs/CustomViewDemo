<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomFilledCircle">

        <attr name="showNumber" format="boolean"/>
        <attr name="colorFill" format="color"/>
        <attr name="colorStroke" format="color"/>
        <attr name="numberFillRange" format="integer"/>

    </declare-styleable>

</resources>
